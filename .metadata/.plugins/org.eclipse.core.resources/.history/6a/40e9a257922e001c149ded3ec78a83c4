
public class p34 {

	public static void main(String[] args) {
		
		int num1 = 10;
		int num2 = 5;
		 System.out.println("num1Mnum2害U贺B衡:");
		 System.out.println("num1+Mnum2单"+(num1+num2));
		 System.out.println("num1-Mnum2单"+(num1-num2));
		 System.out.println("num1*Mnum2单"+(num1*num2));
		 System.out.println("num1/Mnum2单"+(num1/num2));
		 System.out.println("num1%Mnum2单"+(num1%num2));
		 
		 int a = 0;
		 int b = 0;
		 int c = 0;
		 
		 b= a++;
		 c= ++a;
		 
		 System.out.println("]唉bw趣Й幛~患WA┮Hb涵痊"+b);
		 System.out.println("]唉b患Wぇ幛~w取A┮Hc涵痊"+c);
		

	}

}
