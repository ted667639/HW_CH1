
public class p34 {

	public static void main(String[] args) {
		
		int num1 = 10;
		int num2 = 5;
		 System.out.println("num1Mnum2害U贺B衡:");
		 System.out.println("num1+Mnum2单"+(num1+num2));
		 System.out.println("num1-Mnum2单"+(num1-num2));
		 System.out.println("num1*Mnum2单"+(num1*num2));
		 System.out.println("num1/Mnum2单"+(num1/num2));
		 System.out.println("num1%Mnum2单"+(num1%num2));
		

	}

}
